<mjml></mjml>
<mjml owa="desktop">

  <!-- DO NOT CHANGE THE CONTENTS OF THIS FILE UNLESS YOU INTEND ON MODIFYING ALL EMAIL MESSAGE TEMPLATES -->

  <mj-head>

    <!-- Email Preview Text -->
    <mj-preview>
      {{data:email_preview}}
    </mj-preview>
    <mj-include path="./css.mjml" />
    <mj-include path="./attributes.mjml" />

  </mj-head>
  <mj-body>

    <!-- Replace {{param:msg}} with the message name. -->
    <!-- For example, the email "msg/email_change" would result in "./msg/email_change.mjml" -->

    <!-- Sample to try would be `./msg_email_change.mjml'  -->

    <mj-include path="{{param:msg}}" />



    <!-- Footer -->
    <mj-include path="./footer.mjml" />

  </mj-body>
</mjml>