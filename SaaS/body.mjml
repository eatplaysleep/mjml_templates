<!-- This file builds the content card and imports the particular message template, which must include any call to action buttons/links  -->

<mj-section>
  <mj-column mj-class="TIN_content_card">

    <!-- Replace {{data:msg}} with the message name. For example, the email "msg_email_change" could result in "./msg_email_change.mjml" -->
    
    <mj-include path="./{{data:msg}}.mjml" />

  </mj-column>

</mj-section>